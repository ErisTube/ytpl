		"discord bot",
		"discord music",
		"discord player",
		"discord bot on eris",
		"discord player music",
		"eristube",
		"youtube",
		"youtube api",
		"playlists",
		"youtube playlists"
	],
	"author": "ErisTube Inc.",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ErisTube/ytpl/issues"
	},
	"homepage": "https://github.com/ErisTube/ytpl#readme",
	"dependencies": {
		"axios": "^1.7.9",
		"undici": "five"
	},
	"devDependencies": {
		"@types/node": "^22.10.5",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.3"
	},
	"engines": {
		"node": ">=18.20.6"
	},
	"types": "./index.d.ts"
