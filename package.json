{
	"name": "@eristube/ytpl",
	"version": "1.0.0-dev.1737937978",
	"description": "A lightweight and simple YouTube playlist resolver for ErisTube. ",
	"main": "index.js",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"build:del": "rimraf dist && tsc -p tsconfig.json",
		"build:docs": "docgen -s src/*.ts src/**/*.ts -o docs/index.json -c docs/index.yml -g -S 1 -j .jsconfig.json",
		"pub": "npm publish",
		"pub:dev": "npm publish --tag dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ErisTube/ytpl.git"
	},
